from turtle import *
import math

# Name your Turtle.
t = Turtle()

# Set Up your screen and starting position.
setup(500,300)
x_pos = -250
y_pos = -150


### Write your code below:

t.forward(10)
t.right(90)
t.forward(10)
t.right(90)
t.forward(10)
t.right(90)
t.forward(10)
t.right(90)

penup()
goto(100,0)
pendown()
t.forward(100)
t.right(120)
t.forward(100)
t.right(120)
t.forward(100)
t.right(120)
# Close window on click.
exitonclick()
